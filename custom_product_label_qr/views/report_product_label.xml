   <?xml version="1.0" encoding="utf-8"?>
   <odoo>
       <template id="report_product_label_qr_inherited" inherit_id="product.report_product_label">
           <xpath expr="//div[hasclass('col-xs-8')]" position="after">
               <div class="col-xs-4">
                   <img t-att-src="'/report/barcode/?type=QR&amp;value=%s' % product.default_code" style="width:100px;height:100px;"/>
               </div>
           </xpath>
       </template>
   </odoo>
